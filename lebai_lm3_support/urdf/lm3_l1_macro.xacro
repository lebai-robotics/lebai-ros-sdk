<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<!-- Include all common properties -->
  <xacro:include filename="$(find lebai_resources)/urdf/common_colours.xacro"/>
  <xacro:include filename="$(find lebai_resources)/urdf/common_constants.xacro"/>
  <xacro:include filename="$(find lebai_resources)/urdf/common_materials.xacro"/>
<!-- Robot DH parameters define joint-->
<xacro:property name="d1" value="0.21583" />
<xacro:property name="d4" value="0.12063" />
<xacro:property name="d5" value="0.09833" />
<xacro:property name="d6" value="0.08343" />
<xacro:property name="a2" value="-0.38" />
<xacro:property name="a3" value="-0.355"/>
<!-- Robot visual and  collision dir-->
<xacro:property name="visual_base" value="package://lebai_lm3_support/meshes/lm3_l1/visual/base_link.stl" />
<xacro:property name="visual_link1" value="package://lebai_lm3_support/meshes/lm3_l1/visual/link1.stl" />
<xacro:property name="visual_link2" value="package://lebai_lm3_support/meshes/lm3_l1/visual/link2.stl" />
<xacro:property name="visual_link3" value="package://lebai_lm3_support/meshes/lm3_l1/visual/link3.stl" />
<xacro:property name="visual_link4" value="package://lebai_lm3_support/meshes/lm3_l1/visual/link4.stl" />
<xacro:property name="visual_link5" value="package://lebai_lm3_support/meshes/lm3_l1/visual/link5.stl" />
<xacro:property name="visual_link6" value="package://lebai_lm3_support/meshes/lm3_l1/visual/link6.stl" />

<xacro:property name="collision_base" value="package://lebai_lm3_support/meshes/lm3_l1/collision/base_link.stl" />
<xacro:property name="collision_link1" value="package://lebai_lm3_support/meshes/lm3_l1/collision/link1.stl" />
<xacro:property name="collision_link2" value="package://lebai_lm3_support/meshes/lm3_l1/collision/link2.stl" />
<xacro:property name="collision_link3" value="package://lebai_lm3_support/meshes/lm3_l1/collision/link3.stl" />
<xacro:property name="collision_link4" value="package://lebai_lm3_support/meshes/lm3_l1/collision/link4.stl" />
<xacro:property name="collision_link5" value="package://lebai_lm3_support/meshes/lm3_l1/collision/link5.stl" />
<xacro:property name="collision_link6" value="package://lebai_lm3_support/meshes/lm3_l1/collision/link6.stl" />
<!-- Robot link mass-->
<xacro:property name="mass_link1" value="2.31" />
<xacro:property name="mass_link2" value="2.1612" />
<xacro:property name="mass_link3" value="1.8508" />
<xacro:property name="mass_link4" value="0.93256" />
<xacro:property name="mass_link5" value="0.93256" />
<xacro:property name="mass_link6" value="0.5782"/>

<!-- Limit of joints-->
<xacro:property name="joint1_upper_limit" value="6.28" />
<xacro:property name="joint2_upper_limit" value="6.28" />
<xacro:property name="joint3_upper_limit" value="6.28" />
<xacro:property name="joint4_upper_limit" value="6.28" />
<xacro:property name="joint5_upper_limit" value="6.28" />
<xacro:property name="joint6_upper_limit" value="6.28" />


<xacro:property name="joint1_lower_limit" value="-6.28" />
<xacro:property name="joint2_lower_limit" value="-6.28" />
<xacro:property name="joint3_lower_limit" value="-6.28" />
<xacro:property name="joint4_lower_limit" value="-6.28" />
<xacro:property name="joint5_lower_limit" value="-6.28" />
<xacro:property name="joint6_lower_limit" value="-6.28" />

<xacro:property name="joint1_vel_limit" value="3.14" />
<xacro:property name="joint2_vel_limit" value="3.14" />
<xacro:property name="joint3_vel_limit" value="3.14" />
<xacro:property name="joint4_vel_limit" value="3.14" />
<xacro:property name="joint5_vel_limit" value="3.14" />
<xacro:property name="joint6_vel_limit" value="3.14" />

<xacro:macro name="lebai_lm3" params="prefix">


<!-- link list -->
<link name="${prefix}base_link">
	<visual>
		<geometry>
			<mesh filename="${visual_base}"/>
		</geometry>
    	<xacro:material_white name="white"/>
	</visual>
	<collision>
		<geometry>
			<mesh filename="${collision_base}" />
		</geometry>
	</collision>
</link>
<link name="${prefix}link_1">
	<visual>
		<geometry>
			<mesh filename="${visual_link1}" />
		</geometry>
    	<xacro:material_white name="white"/>
	</visual>
	<collision>
		<geometry>
			<mesh filename="${collision_link1}" />
		</geometry>
	</collision>

    <inertial>
      <origin xyz="0.0 -0.004834 -0.08972" rpy="0.0 0.0 0.0"/>
      <mass value="${mass_link1}"/>
      <inertia
	  ixx="0.0093" ixy="0.0" ixz="0.0"
	  iyy="0.00913" iyz="0.0"
	  izz="0.00303"/>
    </inertial>
</link>
<link name="${prefix}link_2">
	<visual>
		<geometry>
			<mesh filename="${visual_link2}" />
		</geometry>
    	<xacro:material_white name="white"/>
	</visual>
	<collision>
		<geometry>
			<mesh filename="${collision_link2}" />
		</geometry>
	</collision>

    <inertial>
      <origin xyz="-0.18435 0.0 0.09428" rpy="0.0 0.0 0.0"/>
      <mass value="${mass_link2}"/>
      <inertia
	  ixx="0.0211" ixy="0.0" ixz="0.03767"
	  iyy="0.1554" iyz="0.0"
	  izz="0.13643"/>
    </inertial>
</link>
<link name="${prefix}link_3">
	<visual>
		<geometry>
			<mesh filename="${visual_link3}" />
		</geometry>
    	<xacro:material_white name="white"/>
	</visual>
	<collision>
		<geometry>
			<mesh filename="${collision_link3}" />
		</geometry>
	</collision>

    <inertial>
      <origin xyz="-0.14386 0.0 0.01892" rpy="0.0 0.0 0.0"/>
      <mass value="${mass_link3}"/>
      <inertia
	  ixx="0.002519" ixy="0.0" ixz="0.00121"
	  iyy="0.08222" iyz="0.0"
	  izz="0.08138"/>
    </inertial> 
</link>

<link name="${prefix}link_4">
	<visual>
		<geometry>
			<mesh filename="${visual_link4}" />
		</geometry>
    	<xacro:material_white name="white"/>
	</visual>
	<collision>
		<geometry>
			<mesh filename="${collision_link4}" />
		</geometry>
	</collision>

    <inertial>
      <origin xyz="0.0 0.011 -0.03772" rpy="0.0 0.0 0.0"/>
      <mass value="${mass_link4}"/>
      <inertia
	  ixx="0.0032645" ixy="0.0" ixz="0.0"
	  iyy="0.003061" iyz="0.0"
	  izz="0.000866"/>
    </inertial>
</link>
<link name="${prefix}link_5">
	<visual>
		<geometry>
			<mesh filename="${visual_link5}" />
		</geometry>
    	<xacro:material_white name="white"/>
	</visual>
	<collision>
		<geometry>
			<mesh filename="${collision_link5}" />
		</geometry>
	</collision>
    <inertial>
      <origin xyz="0.0 -0.011 -0.03772" rpy="0.0 0.0 0.0"/>
      <mass value="${mass_link5}"/>
      <inertia
	  ixx="0.0032645" ixy="0.0" ixz="0.0"
	  iyy="0.003061" iyz="0.0"
	  izz="0.000866"/>
    </inertial>
</link>
<link name="${prefix}link_6">
	<visual>
		<geometry>
			<mesh filename="${visual_link6}" />
		</geometry>
    	<xacro:material_white name="white"/>
	</visual>
	<collision>
		<geometry>
			<mesh filename="${collision_link6}" />
		</geometry>
	</collision>
    <inertial>
      <origin xyz="0 0 -0.04882" rpy="0 0 0"/>
      <mass value="${mass_link6}"/>
      <inertia
	  ixx="0.001883" ixy="0.0" ixz="0.0"
	  iyy="0.0018889" iyz="0.0"
	  izz="0.000404"/>
    </inertial>
</link>

<link name="${prefix}base" />
<link name="${prefix}tool0"/>
<!-- end of link list -->


<!-- joint list -->
<joint name="${prefix}joint_1" type="revolute">
	<parent link="${prefix}base_link"/>
	<child link="${prefix}link_1"/>
	<origin xyz="0.0 0.0 ${d1}" rpy="0.0 0.0 0.0"/>
	<axis xyz="0 0 1" />
	<limit lower="${joint1_lower_limit}" upper="${joint1_upper_limit}" effort="100" velocity="${joint1_vel_limit}" />
</joint>
<joint name="${prefix}joint_2" type="revolute">
	<parent link="${prefix}link_1"/>
	<child link="${prefix}link_2"/>
        <origin xyz="0.0 0.0 0.0" rpy="${PI_2} 0 0"/>
	<axis xyz="0 0 1" />
	<limit lower="${joint2_lower_limit}" upper="${joint2_upper_limit}" effort="100" velocity="${joint2_vel_limit}" />
</joint>
<joint name="${prefix}joint_3" type="revolute">
	<parent link="${prefix}link_2"/>
	<child link="${prefix}link_3"/>
        <origin xyz="${a2} 0 0" rpy="0 0 0"/>
	<axis xyz="0 0 1" />
	<limit lower="${joint3_lower_limit}" upper="${joint3_upper_limit}" effort="100" velocity="${joint3_vel_limit}" />
</joint>
<joint name="${prefix}joint_4" type="revolute">
	<parent link="${prefix}link_3"/>
	<child link="${prefix}link_4"/>
       <origin xyz="${a3} 0.0 ${d4}" rpy="0 0 0"/>
	<axis xyz="0 0 1" />
	<limit lower="${joint4_lower_limit}" upper="${joint4_upper_limit}" effort="100" velocity="${joint4_vel_limit}" />
</joint>
<joint name="${prefix}joint_5" type="revolute">
	<parent link="${prefix}link_4"/>
	<child link="${prefix}link_5"/>
        <origin xyz="0 -${d5} 0" rpy="${PI_2} 0 0"/>
	<axis xyz="0 0 1" />
	<limit lower="${joint5_lower_limit}" upper="${joint5_upper_limit}" effort="5.5" velocity="${joint5_vel_limit}" />
</joint>
<joint name="${prefix}joint_6" type="revolute">
	<parent link="${prefix}link_5"/>
	<child link="${prefix}link_6"/>
        <origin xyz="0  ${d6} 0" rpy="-${PI_2} 0 0"/>
	<axis xyz="0 0 1" />
	<limit lower="${joint6_lower_limit}" upper="${joint6_upper_limit}" effort="4.6" velocity="${joint6_vel_limit}" />
</joint>
<joint name="${prefix}joint_6-tool0" type="fixed">
      <parent link="${prefix}link_6"/>
      <child link="${prefix}tool0"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
</joint>

<joint name="${prefix}base_link-base" type="fixed">
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
</joint>
<!-- end of joint list -->

</xacro:macro>
</robot>
